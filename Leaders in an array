def findTheLeaders(arr):
  # Write your code here
  res=[]
  n=len(arr)
  res.append(arr[n-1])
  mx=arr[n-1]
  for i in range (n-2,-1,-1):
    if arr[i]>mx:
      res.append(arr[i])
      mx=arr[i]
  return res[::-1]
if __name__ == '__main__':
    arr = [12, 27, 14, 11, 25, 6, 7]
    size = len(arr)
    findTheLeaders(arr,size)
