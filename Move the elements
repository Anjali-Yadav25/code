def moveElements(arr, n):
  # Write your code here
  for i in range(n):
    key=arr[i]
    if key>=0:
      j=i
      while j>0:
        if arr[j-1]<0:
          arr[j-1],arr[j]=arr[j],arr[j-1]
        else:
          break
        j-=1
  return arr    
if __name__ == '__main__':
  arr = [-6, -9, 3, 4, -7, 2]
  moveElements(arr,len(arr))
  print(' '.join(str(x) for x in arr))
